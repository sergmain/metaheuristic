    <groupId>ai.metaheuristic</groupId>
    <artifactId>srv</artifactId>
    <version>4.5.0-SNAPSHOT</version>
    <packaging>jar</packaging>

    <name>ai.metaheuristic</name>
    <description>Metaheuristic platform</description>

